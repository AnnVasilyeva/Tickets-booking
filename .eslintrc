{
  "plugins": [
    "jsx-a11y"
  ],
  
  "rules": {
    "jsx-a11y/no-noninteractive-element-interactions": [
      "error",
      {
        "handlers": [
          "onClick",
          "onMouseDown",
          "onMouseUp",
          "onKeyPress",
          "onKeyDown",
          "onKeyUp"
        ]
      }
    ]
    
  },

  "extends": [
    "plugin:jsx-a11y/recommended"
  ]
}

